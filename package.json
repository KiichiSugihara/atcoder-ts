{
  "name": "atcoder-ts",
  "version": "1.0.0",
  "description": "Play AtCoder by TypeScript3.8",
  "scripts": {
    "test": " node test.js",
    "setup": "node setup.js",
    "lint": "eslint \"**/*.ts\"",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KiichiSugihara/atcoder-ts.git"
  },
  "keywords": [
    "TypeScript",
    "JavaScript",
    "Algorithms"
  ],
  "author": "Kiichi Sugihara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KiichiSugihara/atcoder-ts/issues"
  },
  "homepage": "https://github.com/KiichiSugihara/atcoder-ts#readme",
  "dependencies": {
    "@types/node": "^14.0.13",
    "chalk": "^4.1.0",
    "shelljs": "^0.8.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  }
}
