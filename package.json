{
  "name": "atcoder-ts",
  "version": "1.0.0",
  "description": "Play AtCoder by TypeScript5.1",
  "scripts": {
    "test": "node test.js",
    "setup": "node setup.js",
    "lint": "eslint \"**/*.ts\"",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KiichiSugihara/atcoder-ts.git"
  },
  "keywords": [
    "TypeScript",
    "JavaScript",
    "Algorithms"
  ],
  "author": "Kiichi Sugihara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KiichiSugihara/atcoder-ts/issues"
  },
  "homepage": "https://github.com/KiichiSugihara/atcoder-ts#readme",
  "dependencies": {
    "@types/node": "^18.16.1",
    "chalk": "^5.2.0",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.0.5"
  }
}
